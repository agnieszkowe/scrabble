<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>


<Pane xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.scrabble.Controllers.MenuController">
   <children>
      <AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="800.0" prefWidth="1000.0">
         <children>
            <Button fx:id="newGameButton" layoutX="350.0" layoutY="250.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="300.0" text="PLAY" textAlignment="CENTER">
               <font>
                  <Font name="Centaur" size="15.0" />
               </font>
            </Button>
            <Button fx:id="optionsButton" layoutX="350.0" layoutY="350.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="300.0" text="OPTIONS">
               <font>
                  <Font name="Century" size="15.0" />
               </font>
            </Button>
            <Button layoutX="350.0" layoutY="450.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="300.0" text="Button" />
         </children>
      </AnchorPane>
   </children>
</Pane>
